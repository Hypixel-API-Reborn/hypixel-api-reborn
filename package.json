  },
  "license": "MIT",
  "readme": "https://hypixel.stavzdev.me/",
  "repository": {
    "url": "https://github.com/Hypixel-API-Reborn/hypixel-api-reborn"
  },
  "publisher": "StavZ",
  "devDependencies": {
    "@discordjs/docgen": "github:discordjs/docgen",
    "@types/node": "^20.11.19",
    "chai": "^4.3.4",
    "eslint": "^9.3.0",
    "eslint-plugin-jsdoc": "^48.2.7",
    "globals": "^15.3.0",
    "mocha": "^10.4.0",
    "node-env-run": "^4.0.2",
    "path": "^0.12.7",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
